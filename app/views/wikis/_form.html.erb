

    <%= form_for [@wiki, @post] do |f| %>
      <div class="form-group">
        <%= f.label :body %>
        <%= f.text_field :body, class: 'form-control', placeholder: "Enter post title" %>
      </div>

     
          
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>


  